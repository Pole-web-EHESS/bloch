{% for date_horaire in items %}
    <li>Du {{ date_horaire.content['#field_collection_item'].get("field_date").value|date("Y/m/d H:i") }}
        <br/>au {{ date_horaire.content['#field_collection_item'].get("field_date").end_value|date("Y/m/d H:i") }}
        <br/>
        {% if date_horaire.content['#field_collection_item'].get("field_adresse").value[0].family_name is not null %}
            {{ date_horaire.content['#field_collection_item'].get("field_adresse").value[0].family_name }}<br/>
        {% endif %}
        {% if date_horaire.content['#field_collection_item'].get("field_adresse").value[0].address_line1 is not null %}
            {{ date_horaire.content['#field_collection_item'].get("field_adresse").value[0].address_line1 }}
        {% endif %}
        {% if date_horaire.content['#field_collection_item'].get("field_adresse").value[0].address_line2 is not null %}
            {{ date_horaire.content['#field_collection_item'].get("field_adresse").value[0].address_line2 }}
        {% endif %}
        {% if date_horaire.content['#field_collection_item'].get("field_adresse").value[0].locality is not null %}
            - {{ date_horaire.content['#field_collection_item'].get("field_adresse").value[0].locality }}
        {% endif %}
    </li>
{% endfor %}